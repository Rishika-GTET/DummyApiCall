<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/chapterFragment">

    <fragment
        android:id="@+id/chapterFragment"
        android:name="com.example.dummyapicall.ChapterFragment"
        android:label="fragment_chapter"
        tools:layout="@layout/fragment_chapter" >

        <action
            android:id="@+id/action_chapterFragment_to_videoFragment"
            app:destination="@id/videoFragment" />
    </fragment>

    <fragment
        android:id="@+id/videoFragment"
        android:name="com.example.dummyapicall.VideosFragment"
        android:label="fragment_videos"
        tools:layout="@layout/fragment_videos" />


</navigation>